inherit python-wheel

NAME="python-recommonmark"
VERSION=0.5.0
RELEASE=1
CATEGORY="Python"
SUMMARY="Docutils CommonMark extension"
DESCRIPTION="A docutils-compatibility bridge to CommonMark, enabling you to
write CommonMark inside of Docutils & Sphinx projects."

ARCH=noarch

PKG_NAMES+=" recommonmark"
recommonmark_CATEGORY="Text"
recommonmark_CONTENTS="usr/bin/cm2*"
